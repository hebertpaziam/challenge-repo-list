{
    "/github.com/*": {
        "target": "https://github.com/",
        "secure": true,
        "changeOrigin": true,
        "pathRewrite": {
            "/github.com/*": ""
        }
    },
    "/api.github.com/*": {
        "target": "https://api.github.com/",
        "secure": true,
        "changeOrigin": true,
        "pathRewrite": {
            "/api.github.com/*": ""
        }
    }
}
