{
    "workbench.iconTheme": "vscode-icons",
    "vsicons.presets.angular": true,
    "editor.fontSize": 14,
    "editor.tabSize": 4,
    "editor.wordWrap": "off",
    "editor.minimap.enabled": false,
    "files.exclude": {
        "**/.vs": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/*.log": true,
        "**/node_modules": true
    },
    "typescriptHero.imports.organizeOnSave": true,
    "typescriptHero.imports.stringQuoteStyle": "'",
    "typescriptHero.imports.grouping": [
        "/^@angular/",
        "/^rxjs/",
        "/environment$/",
        "/module$/",
        "/routing$/",
        "/guard$/",
        "/service$/",
        "/directive$/",
        "/pipe$/",
        "/component$/",
        "/model$/",
        "/enum$/",
        "Modules",
        "Plains",
        "Workspace",
        "Remaining"
    ]
}
